{
	"manifest_version": 2,
	"name": "58赶集二手房拨号助手 ",
	"version": "1.0.0",
	"description": "58赶集二手房拨号助手",
	"background": {
		"page": "background.html"
	},
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"page_action": {
		"default_icon": "icon.png",
		"default_title": "58赶集二手房拨号助手", // shown in tooltip
		"default_popup": "popup.html"
	},
	"permissions": [
		"cookies",
		"tabs",
		"http://*/*"
	],
	"content_scripts": [
		{
			"matches": [
				"http://*.ganji.com/*",
				"http://*.58.com/*"
			],
			"js": [
				"js/jquery-2.0.0.min.js",
				"content_script.js"
			],
			"run_at": "document_start"
		}
	]
}